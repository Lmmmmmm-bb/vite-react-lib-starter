{
  "name": "vite-react-lib-starter",
  "version": "1.0.2",
  "license": "MIT",
  "packageManager": "pnpm@7.11.0",
  "description": "A react component lib starter",
  "author": {
    "email": "lmmmmmm12138@gmail.com",
    "name": "_lmmmmmm"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Lmmmmmm-bb/vite-react-lib-starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lmmmmmm-bb/vite-react-lib-starter"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "build": "tsc && vite build",
    "build:playground": "pnpm --filter playground build",
    "preview": "pnpm --filter playground preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u",
    "lint:fix": "eslint --ext .tsx,.ts,.js . --fix",
    "lint:check": "pnpm prettier --check .",
    "lint:rewrite": "pnpm prettier --write .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.23.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.5.0",
    "vitest": "^0.23.2"
  },
  "lint-staged": {
    "**/*": "prettier --write . --ignore-unknown",
    "*.{ts,tsx}": "eslint --ext .tsx,.ts,.js . --fix"
  }
}
